{
  "app": {
    "security": {
      "assetProtocol": {
        "enable": true,
        "scope": [
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.png",
          "**/*.webp",
          "**/*.bmp",
          "**/*.tiff",
          "**/*.gif"
        ]
      },
      "csp": "default-src 'self'; style-src 'self' 'unsafe-inline'; img-src 'self' asset: https://asset.localhost/; connect-src ipc: http://ipc.localhost"
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Initiative Tracker: Console",
        "url": "console.html",
        "width": 540
      },
      {
        "label": "presenter",
        "parent": "main",
        "closable": false,
        "visible": false,
        "fullscreen": false,
        "resizable": true,
        "title": "Initiative Tracker: Presenter",
        "width": 800,
        "height": 600,
        "url": "presenter.html"
      },
      {
        "label": "compendium",
        "parent": "main",
        "closable": false,
        "visible": false,
        "fullscreen": false,
        "resizable": true,
        "title": "Initiative Tracker: Compendium",
        "width": 800,
        "height": 600,
        "url": "compendium.html"
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "yarn vite build",
    "beforeDevCommand": "yarn vite",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "identifier": "im.apodaca.initiative-tracker",
  "productName": "Initiative Tracker",
  "version": "1.5.1"
}